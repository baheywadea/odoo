<?xml version="1.0" encoding="utf-8"?>
<odoo>


    <record model="ir.module.category" id="module_category_services_property">
        <field name="name">Realestate</field>
        <field name="parent_id" ref="base.module_category_services"/>
    </record>

    <record id="group_property_user" model="res.groups">
        <field name="name">User</field>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        <field name="category_id" ref="bloopark_realestate.module_category_services_property"/>
    </record>

    <record id="group_property_manager" model="res.groups">
        <field name="name">Administrator</field>
        <field name="implied_ids" eval="[(4, ref('bloopark_realestate.group_property_user'))]"/>
        <field name="category_id" ref="bloopark_realestate.module_category_services_property"/>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin')), (4, ref('base.default_user'))]"/>
    </record>

</odoo>
